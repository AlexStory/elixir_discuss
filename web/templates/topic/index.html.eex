<h2>Topics:</h2>
<ul class="collection" >
  <%= for t <- @topics do %>
    <%= render "topic_item.html",conn: @conn, topic: t %>
  <% end %>
</ul>

<div class="fixed-action-button">
  <%= link to: topic_path(@conn, :new), class: "btn btn-floating light-blue pull-right btn-large waves-effect waves-light" do %>
    <i class="material-icons">add</i>
  <% end %>
</div>
